; Cersyve-9 property point_mass

(declare-const X_0 Real)
(declare-const X_1 Real)
(declare-const X_2 Real)
(declare-const X_3 Real)

(declare-const Y_0 Real)
(declare-const Y_1 Real)

(assert (<= X_0 1))
(assert (>= X_0 -1))
(assert (<= X_1 1))
(assert (>= X_1 -1))
(assert (<= X_2 1))
(assert (>= X_2 -1))
(assert (<= X_3 1))
(assert (>= X_3 -1))

(assert (<= Y_0 0))
(assert (>= Y_1 0))
